@if (Model.NotifyModal?.ShowMessage ?? false)
{
	<script>
		window.onload = function () {
			openModal('#notificacaoModal');
			fillInput(getDate());
		}
	</script>
}
else
{
	<script>
		window.onload = function () {
			fillInput(getDate());
		}
	</script>
}

<partial name="../Modules/_NotifyModal" />